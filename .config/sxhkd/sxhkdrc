super + shift + Return
	$TERMINAL

control + alt + t
    $TERMINAL

super + e
    $FILEMAN

control + shift + Escape
    $TASKMAN

super + {r,p,space}
	rofi -combi-modi window,drun,run -show combi -modi combi

alt + F2
    rofi

Print
    scrot

super + shift + q
    xlogout

control + shift + Print
    scrot -s

XF86Audio{LowerVolume,RaiseVolume,Mute}
	pulsevol.sh {minus,plus,mute}

super + control + h
	pulsevol.sh swport

control + alt + p
    firefox -no-remote -private-window -P proxied

control + alt + b
    firefox -no-remote -P default
